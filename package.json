{
  "name": "web-nextjs-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0 <23",
    "npm": ">=10.0.0 <12"
  },
  "scripts": {
    "dev-localbackend": "env-cmd -e localbackend,local --use-shell 'NEXT_PUBLIC_APP_BUILD_SHA=$(git rev-parse HEAD) next dev'",
    "dev": "env-cmd -e dev,local --use-shell 'NEXT_PUBLIC_APP_BUILD_SHA=$(git rev-parse HEAD) next dev'",
    "build:prod": "env-cmd -e prod --use-shell 'NEXT_PUBLIC_APP_BUILD_SHA=$(git rev-parse HEAD) next build'",
    "build:acc": "env-cmd -e acc --use-shell 'NEXT_PUBLIC_APP_BUILD_SHA=$(git rev-parse HEAD) next build'",
    "build:develop": "env-cmd -e test --use-shell 'NEXT_PUBLIC_APP_BUILD_SHA=$(git rev-parse HEAD) next build'",
    "start:prod": "env-cmd -e prod next start",
    "start:acc": "env-cmd -e acc next start",
    "start:develop": "env-cmd -e test next start",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.1.1",
    "@chakra-ui/icon": "^3.0.15",
    "@chakra-ui/next-js": "^2.4.2",
    "@chakra-ui/react": "^2.10.9",
    "@chakra-ui/system": "^2.6.2",
    "@chakra-ui/utils": "^2.0.14",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/utilities": "^3.2.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@reduxjs/toolkit": "^1.9.1",
    "@sentry/nextjs": "^9.14.0",
    "axios": "^1.2.1",
    "fast-deep-equal": "^3.1.3",
    "firebase": "^11.2.0",
    "framer-motion": "^7.10.3",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "next": "^15.5.2",
    "next-redux-wrapper": "^8.1.0",
    "next-seo": "^6.8.0",
    "next-translate": "^2.6.2",
    "react": "^18.3.1",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-redux": "^8.0.5",
    "react-select": "^5.7.0",
    "react-to-print": "^2.14.12",
    "redux-persist": "^6.0.0",
    "styled-jsx": "^5.1.1",
    "use-debounce": "^9.0.2"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.9.4",
    "@playwright/test": "^1.33.0",
    "@types/luxon": "^3.2.0",
    "@types/node": "^22.18.0",
    "@types/react": "^18.0.26",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.29.0",
    "eslint-config-next": "^14.2.23",
    "gts": "^3.1.1",
    "husky": "^8.0.0",
    "next-translate-plugin": "^2.6.2",
    "typescript": "^5.9.2"
  }
}
